 "client_script": "//whatsapp_number, from_webform=False, event='', are_you_attending_event='Yes', visitor_count=1\n\nfrappe.web_form.validate = () => {\n       frappe.call({\n\t\t\t\tmethod: \"visitor_management.visitor_management.custom.customer.duplicate_entry\",\n\t\t\t\targs: {\n\t\t\t\t\twhatsapp_number: frappe.web_form.fields_dict.whatsapp_number.value?frappe.web_form.fields_dict.whatsapp_number.value:'',\n\t\t\t\t\tfrom_webform:1,\n\t\t\t\t\tevent:frappe.web_form.fields_dict.event.value?frappe.web_form.fields_dict.event.value:'EV00002',\n\t\t\t\t\tare_you_attending_event:'Yes',\n\t\t\t\t\tvisitor_count:frappe.web_form.fields_dict.visitor_count.value?frappe.web_form.fields_dict.visitor_count.value:1\n\t\t\t\t\t\n\t\t\t\t},\n\t\t\t\tcallback: function(r){\n\t\t\t\t\tlet res=r.message\n\t\t\t\t    if(res){\n\t\t\t\t\t\tlocation.pathname=res['error']\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t})\n       return true\n}\nfrappe.web_form.after_save = () => {\n       location.pathname='regcomp'\n}\n",
